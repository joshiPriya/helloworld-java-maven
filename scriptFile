pipeline { 
    agent any  
    tools { 
        maven 'maven3' 
        jdk 'java' 
    }
    
   
    stages { 
        stage('check') { 
            steps { 
               echo 'Initial check' 
            }
        }
        
      stage('code checkout') {
            steps {
            checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/joshiPriya/helloworld-java-maven.git']]])
            echo 'checkout done for master'
            }
          }  
		  
		  
          
          stage('Deliver before deployment-sonarqube analysis') {
            when {
                branch 'fea1'
            }
            steps {
                 withSonarQubeEnv('sonar') { 
                 
                       sh 'mvn sonar:sonar'
                       
                    }   
            }
        }
        
         stage('Deliver after sonarqube analysis') {
            when {
                branch 'newb'
            }
            steps {
                
                sh 'mvn deploy' 
            
            }
        }
        
         stage('post check') { 
            steps { 
               echo 'Al done' 
            }
        }
          
 /*    stage('SonarQube Analysis') {
            steps {
             withSonarQubeEnv('sonar') { 
                 
                       sh 'mvn sonar:sonar'
                       
                    }   
       
    }
    } 
        
         stage ('Build') {
            steps {
                sh 'mvn deploy' 
            }
          } */
          }
    }
