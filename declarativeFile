pipeline { 
    agent any  
    tools { 
        maven 'maven3' 
        jdk 'java' 
    }
    
   
    stages { 
        stage('check') { 
            steps { 
               echo 'This is check.' 
            }
        }
         stage ('Build') {
            steps {
                sh 'mvn deploy' 
            }
          }
          }
    }

