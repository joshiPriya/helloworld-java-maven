pipeline { 
    agent any  
    tools { 
        maven 'maven3' 
        jdk 'java' 
    }
    
   
    stages { 
        stage('check') { 
            steps { 
               echo 'Initial check' 
            }
        }
        
      stage('code checkout') {
            steps {
            git 'https://github.com/joshiPriya/helloworld-java-maven.git'
            echo 'checkout done'
            }
          }  
          
     stage('SonarQube Analysis') {
             withSonarQubeEnv('sonar') { 
                 
                       sh 'mvn sonar:sonar'
       
    }
    }
        
         stage ('Build') {
            steps {
                sh 'mvn deploy' 
            }
          }
          }
    }

