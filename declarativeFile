pipeline { 
    agent any  
    tools { 
        maven 'maven3' 
        jdk 'java' 
    }
    
   
    stages { 
        stage('check') { 
            steps { 
               echo 'This is check.' 
            }
        }
        
      stage('code checkout') {
            steps {
            git 'https://github.com/joshiPriya/helloworld-java-maven.git'
            echo 'checkout done'
            }
          }  
        
         stage ('Build') {
            steps {
                sh 'mvn deploy' 
            }
          }
          }
    }

